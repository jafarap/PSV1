<script src="/Scripts/jquery-1.8.2.min.js"></script>
<script src="/Scripts/jquery-ui-1.8.24.js"></script>
<script src="/Scripts/jquery.validate.js"></script>
<script src="/Scripts/jquery.validate.unobtrusive.js"></script>
<script src="/Scripts/ProjectFile/CustomJQuery.js"></script>
<script type="text/javascript">
    $('#RoleCode').val('');
    $('#ConfirmEmailId').val('');
    $('#EmailId').val('');
    $('#CRN').val('');
    $('input[type=checkbox]:checked').attr('checked', false);
    var URL = "Account/Signup?RoleCode=APP";
    LoginDivShowHide();

    var defaultRangeValidator = $.validator.methods.range;
    $.validator.methods.range = function (value, element, param) {
        if (element.type === 'checkbox') {
            // if it's a checkbox return true if it is checked
            return element.checked;
        } else {
            // otherwise run the default validation function
            return defaultRangeValidator.call(this, value, element, param);
        }
    }

    $(document).ready(function () {
        if ($("input[name='RoleCode']:checked").val() == "APP") {
            $('#divCRN').css("display", "block");
        }
        $("#applicant").click(function () {
            $(".disabled").css("opacity", "1.0");
            $(".disabled").css('pointer-events', 'auto');
            $(".disabled").css("cursor", "pointer");
            $('#divCRN').css("display", "block");
           // $("#TextUIN").css("display", "block");

        });

        $("#facilitate").click(function () {
            $(".disabled").css("opacity", "0.6");
            $(".disabled").css("pointer-events", "none");
            $(".disabled").css("cursor", "default");
            $('#divCRN').css("display", "none");
            $('#CRN').val('');
           // $("#TextUIN").css("display", "none");
        });
    });


</script>
